{% if pageTitle is defined %}
    <h1 class="subheader-title mb-2">{{ pageTitle }}</h1>
{% endif %}

{% if pageTitle is defined %}
    <nav class="app-breadcrumb" aria-label="breadcrumb">
        <ol class="breadcrumb ms-0 text-muted mb-0">
            {% if pageSubTitle1 is defined %}
                <li class="breadcrumb-item">{{ pageSubTitle1 }}</li>
            {% endif %}
            {% if pageSubTitle2 is defined %}
                <li class="breadcrumb-item">{{ pageSubTitle2 }}</li>
            {% endif %}
            <li class="breadcrumb-item active" aria-current="page">{{ pageTitle }}</li>
        </ol>
    </nav>
{% endif %}

{% if pageSubText is defined and pageSubText != 'false' %}
    <h6 class="mt-3 mb-4 fst-italic">{{ pageSubText|raw }}</h6>
{% endif %}
